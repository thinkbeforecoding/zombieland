pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- tiles

function limit(x,mn,mx)
	return min(max(x,mn),mx)
end

function istile(x,y,t)
	local ln = limit(y/8,-1,128)
	local cl = limit(x/8,0,128)
 local c = mget(cl,ln)
 if (band(fget(c),t)!=0)then
 	return true
 else
  return false
 end
end

function isfloor(x,y)
 return istile(x,y,1)
end

function find_floor(x,y)
 local sy = flr(y/8)*8+8
 while (not isfloor(x,sy)) and sy < 8*64 do
 	sy+=8
 end
 return sy
end

function left_wall(x,y)
 local sx = flr((x+1)/8)*8

  if isfloor(sx,y+1) 
     or isfloor(sx,y+7) then
  	return sx+7
  else
   return x
  end
end
 	
function right_wall(x,y)
 local sx = flr((x+6)/8)*8

  if isfloor(sx,y+1) 
     or isfloor(sx,y+7) then
  	return sx-6
  else
   return x
  end
end

function iscoin(x,y)
 return istile(x,y,0x80)
end

function removecoin(x,y)
 mset(x/8,y/8,13)
end
-->8
-- main functions
x0 = 8
y0 = 48

x = x0
y = y0
jump=false
vy = 0
score=0
coins=0
right=true

function _update()
if (btn(⬅️)) then 
	x-=1
	right=false
	end
if (btn(➡️)) then 
	x+=1
	right=true
	end
if (btn(❎) and not jump) then
		vy = -4
		jump = true
end

for m in all(monsters) do
	m.update()
end

x=left_wall(x,y)
x=right_wall(x,y)

vy+=0.3
ny=y+vy
sy=min(find_floor(x+2,y),find_floor(x+5,y))

if ny>=sy-8 then
	jump=false
	vy=0
	ny=sy-8
else
 jump=true
end
x = min(max(x,0),127*8)
y = ny

if iscoin(x+4,y+4) then
		removecoin(x+4,y+4)
		score+=10
		coins+=1
		sfx(5)
end

local m = collision_monster(x,y)
if m != nil then
	sfx(2)
	x=x0
	y=y0
end
end



function _draw() 
cls(5)
sx = min(max(x-64,0),(128-16)*8)
sy = min(max(y-80,0),(128-16)*8) 
camera(sx,sy)

map(0,0,0,0,128,128)

if right then
spr(4,x,y)
else
spr(5,x,y)
end

for m in all(monsters) do
	m.draw()
end

camera(0,0)
prints("score:"..score,4,4)
prints("coins:"..coins,90,4)

prints("x:"..x,4,120)
prints("y:"..y,40,120)


--fillp(0b1011001110001100)
--rectfill(0,0,127,127,0x89)
end

function prints(t,x,y)
	print(t,x+1,y+1,0)
	print(t,x,y,10)
end


-->8
-- monsters

monsters={}




function new_zombie(i,j,gun)
	local z = {x=i*8,y=j*8,gun=gun,v=0.3}
	add(monsters,z)
	local function draw()
		local s = 8
		if z.gun then
		 	s=15
		end
		if z.v < 0 then
		 s-=1
		end

		spr(s,z.x,z.y)
	end
	local function update()
		if z.v > 0 then
			if isfloor(z.x+7,z.y) or not isfloor(z.x+7,z.y+8) then
			   z.v=-z.v
			end
		else
			if isfloor(z.x-1,z.y) or not isfloor(z.x-1,z.y+8) then
			   z.v=-z.v
			end		
		end
		z.x+=z.v
		
	end
	z.draw = draw
	z.update = update

end



function new_ghost(i,j)
	local z = {x=i*8,y=j*8,t=0,v=0.3}
	add(monsters,z)
	local function draw()
		local s = 10
		if z.v < 0 then
		 s-=1
		end

		spr(s,z.x,z.y+1.5*sin(z.t/50))
	end
	local function update()
  z.t+=1
		if z.v > 0 then
			if isfloor(z.x+7,z.y) or not isfloor(z.x+7,z.y+8) then
			   z.v=-z.v
			end
		else
			if isfloor(z.x-1,z.y) or not isfloor(z.x-1,z.y+8) then
			   z.v=-z.v
			end		
		end
		z.x+=z.v
		
	end

	z.draw = draw
	z.update = update

end	
	

for j=0,31 do
for i=0,127 do

local s=mget(i,j)
local f=fget(s)
if band(f,0x2)!=0 then
	new_zombie(i,j,band(f,8)!=0)
	mset(i,j,0)
elseif band(f,0x4)!=0 then
	new_ghost(i,j)
	mset(i,j,0)
end

end
end

function collision_monster(x,y)
	for m in all(monsters) do
		if abs(m.x - x)<6 and
		   abs(m.y - y)<6 then
		   return m
		end 
	end
	return nil
end

